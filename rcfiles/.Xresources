*color0: #121858
! bright_black
*color8: #2b3171
! red
*color1: #b6302d
! bright_red
*color9: #ecb3b2
! green
*color2: #2db630
! bright_green
*color10: #b2ecb3
! yellow
*color3: #b4b62d
! bright_yellow
*color11: #ebecb2
! blue
*color4: #302db6
! bright_blue
*color12: #b3b2ec
! magenta
*color5: #b62db4
! bright_magenta
*color13: #ecb2eb
