xterm*dynamiccolors:      true
xterm*visualBell:         off
xterm*highlightSelection: true

URxvt*termName: screen-256color
URxvt*iso14755: false
URxvt*iso14755_52: false
URxvt.imLocale: pl_PL.UTF-8
URxvt.imFont: xft:Inconsolata:style=Regular:size=15

URxvt*visualBell: off
URxvt*urgentOnBell: true

URxvt.font: xft:Inconsolata:style=Regular:size=15
URxvt.boldFont: xft:Inconsolata:style=Bold:size=15
URxvt.italicFont: xft:Inconsolata:style=Italic:size=15
URxvt.boldItalicFont: xft:Inconsolata:style=Bold Italic:size=15

URxvt.xftAntialias: true
URxvt.xftHinting: true
URxvt.letterSpace: -1

Xft.dpi: 96
Xft.antialias: true
Xft.rgba: rgb
Xft.hinting: true

URxvt*scrollTtyOutput: false
URxvt.clipboard.autocopy: true
URxvt*scrollBar: false
! URxvt*depth: 80
URxvt*dynamicColors: on

URxvt.copyCommand: xclip -i -selection clipboard
URxvt.pasteCommand: xclip -o -selection clipboard
URxvt.urlLauncher: chrome
URxvt.underlineURLs: true
URxvt.urlButton: 1
